modules = ["php-8.2", "nodejs-20", "bash", "web"]
[agent]
expertMode = true
integrations = ["sendgrid:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["openssh", "unzip"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "dev-server"

[[workflows.workflow]]
name = "dev-server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "php -S 0.0.0.0:5000 server.php"
waitForPort = 5000

